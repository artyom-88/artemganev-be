    "yarn-audit-ci": "^1.2.0"
  },
  "resolutions": {
    "node-forge": ">=1.3.0",
    "nth-check": ">=2.0.1",
    "minimist": ">=1.2.6"
  },
  "lint-staged": {
    "src/**/*.{scss,css,json,yml,html}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
